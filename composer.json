{
    "name": "dbarzin/mercator",
    "type": "project",
    "description": "Cartographie du systeme d'information / Mapping the information system",
    "keywords": [
        "information system",
        "cartography",
        "oiv",
        "security tool",
        "anssi",
        "urbanisation" 
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "bugsnag/bugsnag-laravel": "^2.18",
        "directorytree/ldaprecord-laravel": "^2.5",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/passport": "^10.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "phpoffice/phpspreadsheet": "^1.14",
        "phpoffice/phpword": "^0.18.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.11"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.3.6",
        "laravel/dusk": "^6.15",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.6",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
